comparisons <- c( "APA_vs_BAM", "APA_vs_BAVA", "APA_vs_COB", "APA_vs_IVI", "APA_vs_MOV", "BAM_vs_BAVA", "BAM_vs_COB", "BAM_vs_IVI", "BAM_vs_MOV", "BAVA_vs_COB", "BAVA_vs_IVI", "BAVA_vs_MOV", "COB_vs_IVI", "COB_vs_MOV", "IVI_vs_MOV" )
file_names <- paste( "Fst_", comparisons, ".windowed5kb.step5kb.weir.fst", sep='', coll='' )
fsts <- data.frame()
for (i in 1:15) {
filename <- file_names[i]
filesize <- as.numeric( strsplit( gsub( '[ ]+', " ", system( paste( "wc -l ", filename, coll='', sep='' ),
intern=T )), " ")[[1]][2])
readin <- read.table( filename, header=TRUE, nrows=filesize  )
readin <- transmute( readin, scaf=factor( CHROM ), bin=as.numeric( BIN_START ), n_var=as.numeric( N_VARIANTS ),
fst=as.numeric( WEIGHTED_FST ), comp=factor( rep( comparisons[i], nrow( readin ) ) ),
scaf_bin=factor( paste( scaf, '_', bin, sep='', coll='' ) ),
scaf_comp=factor( paste( scaf, '_', comp, sep='', coll='' )) )  %>%  filter( fst > 0 )
fsts <- rbind( fsts, readin )
}
# most scafs have only 1 bin. 3rd quartile at 2!
by_scaf <- group_by( fsts, scaf ) %>% summarise( n_bins=n_distinct( scaf_bin ) ) %>% filter( n_bins >2 )
fltr_fsts <- filter( fsts, scaf %in% droplevels( by_scaf$scaf ) ) %>%  # dumping shorter scafs
filter( scaf != "Scaffold1910" & scaf != "Scaffold3315" &
scaf != "Scaffold3294" & scaf != "Scaffold24" )  %>% # dumping all 4 scafs with contamination
group_by( comp ) %>% filter( fst > quantile( fst, probs=0.95 ) ) # dumping low fst scafs *within* comp.
fltr_fsts <- droplevels( fltr_fsts )
head(fltr_fsts)
fltr_fsts$scaf_bin[ fltr_fsts$comp == "BAVA_vs_MOV" ]
fltr_fsts <- droplevels( fltr_fsts[ fltr_fsts$scaf_bin %in% select_scaf_bins ,] )
select_scaf_bins <- intersect( setdiff( intersect(   # selecting the bins that make the cut in:
fltr_fsts$scaf_bin[ fltr_fsts$comp == "BAVA_vs_MOV" ], fltr_fsts$scaf_bin[ fltr_fsts$comp == "BAVA_vs_IVI" ] ), # absent v. present comparisons
fltr_fsts$scaf_bin[ fltr_fsts$comp == "IVI_vs_MOV" ] ), # NOT in present v. present...
fltr_fsts$scaf_bin[ fltr_fsts$comp == "APA_vs_BAM" ] )  # and in mostly present vs. mostly absent.
fltr_fsts <- droplevels( fltr_fsts[ fltr_fsts$scaf_bin %in% select_scaf_bins ,] )
dim(fltr_fsts)
